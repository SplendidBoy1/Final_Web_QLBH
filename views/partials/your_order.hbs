<div class="card">
    <div class="card-header">
        Your Order
    </div>
    <div class="card-body">
        <table class="table">
            <tr>
                <th scope="col">Order ID</th>
                <th scope="col">Date</th>
                <th scope="col">Total Amount</th>                             
            </tr>
            {{#each orders}}
            <tr>
                <td><a href="/order/{{this.OrderID}}">#{{this.OrderID}}</a></td>
                <td>{{this.OrderDate}}</td>
                <td>{{this.Total}}</td>
            </tr>
            {{/each}}
        </table>        

    </div>

</div>